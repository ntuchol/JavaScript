<html>
<head>
    <title>My Javascript Code</title>

    <script type = "text/javascript">
    
    var register = prompt("You can't eneter, please type a valid password");

    function success() {
      document.write("Correct password!  You have entered the site");
    }
    function failed() {
      document.write("Wrong password, you can't enter");
    }

    if(register != "") {
      var user = prompt("You have registered, please confirm your password");
      if (register == user) {
        success();
      }
      else{
        failed();
      }
      else {
        alert("ERROR: You have to type something as password");
      }
      
      </script>
    </head>

